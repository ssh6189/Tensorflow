    "#텐서플로우 읽어 들이기\n",
    "import tensorflow as tf\n",
    "\n",
    "#상수 정의하기\n",
    "a = tf.constant(100)\n",
    "b = tf.constant(30)\n",
    "\n",
    "#연산 정의하기\n",
    "add_op = a + b\n",
    "\n",
    "#세션 시작하기\n",
    "sess = tf.Session()\n",
    "res = sess.run(add_op)\n",
    "print(res)"
   ]
  }
